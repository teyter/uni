public class Medaltol5 {
    public static void main(String[] args) {
        double a = Double.parseDouble(args[0]);
        double b = Double.parseDouble(args[1]);
        double c = Double.parseDouble(args[2]);
        double d = Double.parseDouble(args[3]);
        double e = Double.parseDouble(args[4]);

        System.out.println("Venjulegt meðaltal: " + (a+b+c+d+e)/5);
        System.out.println("Faldmeðaltal: " + Math.pow(a,1.0/5)*Math.pow(b,1.0/5)*Math.pow(c,1.0/5)*Math.pow(d,1.0/5)*Math.pow(e,1.0/5));
        System.out.println("Þýtt meðaltal: " + (5/(a+b+c+d+e)));
    }
}
